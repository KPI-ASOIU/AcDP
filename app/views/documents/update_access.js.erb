$('#modal_search_user').modal('hide');

<% if @access_type == 1 %>
$('#docs-edit-access-list-<%= @doc.id %>').html('<%= escape_javascript(render :partial => 'access_list', :locals => { doc: @doc, access_type: @access_type }) %>');
<% else %>
$('#docs-view-access-list-<%= @doc.id %>').html('<%= escape_javascript(render :partial => 'access_list', :locals => { doc: @doc, access_type: @access_type }) %>');
<% end %>